{
  "QC": {
    "key_colname": "manifest",
    "factor_vars": [
      "individual"
    ],
    "min_library_size": 300,
    "max_library_size": "adaptive",
    "min_features": 100,
    "max_features": "adaptive",
    "max_mito": 0.1,
    "min_ribo": 0,
    "max_ribo": 1,
    "min_counts": 2,
    "min_cells": 2,
    "drop_unmapped": true,
    "drop_mito": true,
    "drop_ribo": true,
    "nmads": 3.5
  },
  "findcells": {
    "find_cells": false,
    "lower": 100,
    "retain": 300,
    "alpha_cutoff": 0.001,
    "niters": 10000
  },
  "singlets": {
    "find_singlets": false,
    "singlets_method": "doubletfinder",
    "vars_to_regress_out": [
      "nCount_RNA", 
      "pc_mito"
    ],
    "pca_dims": 10,
    "var_features": 2000,
    "doublet_rate": 0,
    "dpk": 8,
    "pK": "NULL"
  },
  "integrate": {
    "method": "Liger",
    "unique_id_var": "manifest",
    "take_gene_union": false,
    "remove_missing": true,
    "num_genes": 3000,
    "combine": "union",
    "keep_unique": false,
    "capitalize": false,
    "use_cols": true,
    "k": 30,
    "lambda": 5.0,
    "thresh": 0.0001,
    "max_iters": 100,
    "nrep": 1,
    "rand_seed": 1,
    "knn_k": 20,
    "k2": 500,
    "prune_thresh": 0.2,
    "ref_dataset": "NULL",
    "min_cells": 2,
    "quantiles": 50,
    "nstart": 10,
    "resolution": 1,
    "dims_use": "NULL",
    "dist_use": "CR",
    "center": false,
    "small_clust_thresh": 0
  },
  "integration_report": {
    "categorical_covariates": [
      "individual", 
      "diagnosis",
      "sex"
    ],
    "input_reduced_dim": "UMAP"
  },
  "merge": {
    "plot_vars": [
      "total_features_by_counts",
      "total_counts",
      "pc_mito",
      "pc_ribo"
    ],
    "facet_vars": ["null"],
    "outlier_vars": [
      "total_features_by_counts",
      "total_counts"
    ]
  },
  "reddim": {
    "input_reduced_dim": [
      "PCA",
      "Liger"
    ],
    "reduction_methods": [
      "tSNE", 
      "UMAP", 
      "UMAP3D"
    ],
    "vars_to_regress_out": [
      "nCount_RNA", 
      "pc_mito"
    ],
    "pca_dims": 30,
    "n_neighbors": 30,
    "n_components": 2,
    "init": "pca",
    "metric": "euclidean",
    "n_epochs": 500,
    "learning_rate": 1,
    "min_dist": 0.3,
    "spread": 0.3,
    "set_op_mix_ratio": 1,
    "local_connectivity": 1,
    "repulsion_strength": 1,
    "negative_sample_rate": 5,
    "fast_sgd": false
  },
  "cluster": {
    "cluster_method": "leiden",
    "reduction_method": "UMAP_Liger",
    "res": 0.00001,
    "k": 50,
    "louvain_iter": 1
  },
  "mapct": {
    "clusters_colname": "clusters",
    "cells_to_sample": 10000
  },
  "de": {
    "de_method": [
      "MASTZLM"
    ],
    "mast_method": "bayesglm",
    "min_counts": 110,
    "min_cells_pc": 0.1,
    "rescale_numerics": true,
    "pseudobulk": true,
    "celltype_var": "cluster_celltype",
    "sample_var": "manifest",
    "dependent_var": "diagnosis",
    "ref_class": "Controls",
    "confounding_vars": [
      "sex", "age"
    ],
    "random_effects_var": "null",
    "fc_threshold": 1.1,
    "pval_cutoff": 0.05,
    "force_run": false
  },
   "IPA": {
    "reference_file": "null",
    "enrichment_tool": [
      "WebGestaltR"
    ],
    "enrichment_method": "ORA",
    "enrichment_database": [
      "GO_Biological_Process"
    ],
    "is_output": true,
    "output_dir": "."
  },
  "plot_reddim_genes": {
    "reduction_methods": [
      "UMAP_Liger"
    ]
  },
  "dirichlet": {
    "unique_id_var": "manifest",
    "celltype_var": "cluster_celltype",
    "dependent_var": "diagnosis",
    "ref_class": "Controls",
    "var_order": [
      "null"
    ]
  },
  "celltype_metrics": {
    "unique_id_var": "manifest",
    "clusters_colname": "clusters",
    "celltype_var": "cluster_celltype",
    "facet_vars": [
      "manifest",
      "diagnosis",
      "sex"
    ],
    "metric_vars": [
      "pc_mito",
      "pc_ribo",
      "total_counts",
      "total_features_by_counts"
    ]
  }
}
